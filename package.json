{
  "name": "discord-ai-bot",
  "version": "1.0.0",
  "description": "An AI-powered Discord bot built using Bun, TypeScript and ESLint.",
  "license": "MIT",
  "private": true,
  "type": "module",
  "homepage": "https://github.com/techwithanirudh/discord-ai-bot",
  "repository": {
    "type": "git",
    "url": "https://github.com/techwithanirudh/discord-ai-bot.git"
  },
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "format": "prettier --cache --write --list-different --ignore-path .gitignore --ignore-path .prettierignore .",
    "format:check": "prettier --cache --check --ignore-path .gitignore --ignore-path .prettierignore .",
    "deploy": "bun run src/deploy-commands.ts",
    "typecheck": "tsc --noEmit",
    "check": "bun lint && bun typecheck && bun format:check && bun check:spelling",
    "check:spelling": "cspell -c .cspell.json --no-progress --no-summary --no-must-find-files --unique",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-sdk-tool/parser": "^2.1.7",
    "@ai-sdk/cohere": "^2.0.13",
    "@ai-sdk/google": "^2.0.18",
    "@ai-sdk/openai": "^2.0.46",
    "@ai-sdk/openai-compatible": "^1.0.20",
    "@cspell/dict-bash": "^4.2.1",
    "@cspell/dict-redis": "^1.0.7",
    "@date-fns/tz": "^1.4.1",
    "@deepgram/sdk": "^4.11.2",
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.18.0",
    "@elevenlabs/elevenlabs-js": "^2.18.0",
    "@neondatabase/serverless": "^1.0.2",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@opentelemetry/api-logs": "^0.206.0",
    "@opentelemetry/auto-instrumentations-node": "^0.65.0",
    "@opentelemetry/instrumentation": "^0.206.0",
    "@opentelemetry/sdk-logs": "^0.206.0",
    "@opentelemetry/sdk-node": "^0.206.0",
    "@pinecone-database/pinecone": "^6.1.2",
    "@t3-oss/env-core": "^0.13.8",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.5",
    "@vercel/functions": "^2.2.13",
    "@vercel/otel": "^2.0.1",
    "ai": "^5.0.64",
    "ai-fallback": "^1.0.7",
    "compromise": "^14.14.4",
    "cspell": "^9.2.1",
    "date-fns": "^4.1.0",
    "discord.js": "^14.23.2",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.6",
    "exa-js": "^1.10.1",
    "ffmpeg-static": "^5.2.0",
    "fuse.js": "^7.1.0",
    "jsonrepair": "^3.13.1",
    "langfuse-vercel": "^3.38.5",
    "libsodium-wrappers": "^0.7.15",
    "lint-staged": "^16.2.3",
    "node-crc": "^1.3.2",
    "pino": "^9.13.1",
    "pino-pretty": "^13.1.1",
    "prism-media": "^2.0.0-alpha.0",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/bun": "latest",
    "@types/node": "^22.18.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "drizzle-kit": "^0.31.5",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.6.0",
    "husky": "^9.1.7",
    "lefthook": "^1.13.6",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^4.3.0"
  },
  "engines": {
    "node": ">=22"
  },
  "lint-staged": {
    "*.{cjs,mjs,js,jsx,cts,mts,ts,tsx,json}": "eslint --fix .",
    "**/*": [
      "prettier --write --ignore-unknown",
      "bun check:spelling"
    ]
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "trustedDependencies": [
    "@discordjs/opus",
    "esbuild",
    "ffmpeg-static",
    "lefthook",
    "node-crc",
    "unrs-resolver"
  ]
}

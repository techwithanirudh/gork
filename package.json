{
  "name": "discord-ai-bot",
  "version": "1.0.0",
  "description": "An AI-powered Discord bot built using Bun, TypeScript and ESLint.",
  "license": "MIT",
  "private": true,
  "type": "module",
  "homepage": "https://github.com/techwithanirudh/discord-ai-bot",
  "repository": {
    "type": "git",
    "url": "https://github.com/techwithanirudh/discord-ai-bot.git"
  },
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "format": "prettier --cache --write --list-different --ignore-path .gitignore --ignore-path .prettierignore .",
    "format:check": "prettier --cache --check --ignore-path .gitignore --ignore-path .prettierignore .",
    "deploy": "bun run src/deploy-commands.ts",
    "typecheck": "tsc --noEmit",
    "check": "bun lint && bun typecheck && bun format:check && bun check:spelling",
    "check:spelling": "cspell -c .cspell.json --no-progress --no-summary --no-must-find-files --unique",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-sdk-tool/parser": "^3.3.1",
    "@ai-sdk-tools/memory": "^1.2.0",
    "@ai-sdk/cohere": "^3.0.17",
    "@ai-sdk/google": "^3.0.19",
    "@ai-sdk/openai": "^3.0.24",
    "@ai-sdk/openai-compatible": "^2.0.25",
    "@cspell/dict-bash": "^4.2.2",
    "@cspell/dict-redis": "^1.0.7",
    "@date-fns/tz": "^1.4.1",
    "@deepgram/sdk": "^4.11.3",
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "@elevenlabs/elevenlabs-js": "^2.33.0",
    "@exalabs/ai-sdk": "^1.0.5",
    "@neondatabase/serverless": "^1.0.2",
    "@openrouter/ai-sdk-provider": "^2.1.1",
    "@opentelemetry/api-logs": "^0.211.0",
    "@opentelemetry/instrumentation": "^0.211.0",
    "@opentelemetry/sdk-logs": "^0.211.0",
    "@pinecone-database/pinecone": "^7.0.0",
    "@snazzah/davey": "^0.1.9",
    "@t3-oss/env-core": "^0.13.10",
    "@vercel/functions": "^3.4.0",
    "@vercel/otel": "^2.1.0",
    "ai": "^6.0.66",
    "ai-retry": "^1.2.0",
    "compromise": "^14.14.5",
    "cspell": "^9.6.2",
    "date-fns": "^4.1.0",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "ffmpeg-static": "^5.3.0",
    "fuse.js": "^7.1.0",
    "jsonrepair": "^3.13.2",
    "langfuse-vercel": "^3.38.6",
    "libsodium-wrappers": "^0.8.2",
    "lint-staged": "^16.2.7",
    "mem0ai": "^2.2.1",
    "node-crc": "^4.0.0",
    "opusscript": "^0.1.1",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "prism-media": "^1.3.5",
    "redis": "^5.10.0",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@types/bun": "^1.3.8",
    "@types/node": "^25.1.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-promise": "^7.2.1",
    "husky": "^9.1.7",
    "lefthook": "^2.0.16",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0"
  },
  "engines": {
    "node": ">=22"
  },
  "lint-staged": {
    "*.{cjs,mjs,js,jsx,cts,mts,ts,tsx,json}": "eslint --fix .",
    "**/*": [
      "prettier --write --ignore-unknown",
      "bun check:spelling"
    ]
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "trustedDependencies": [
    "@discordjs/opus",
    "esbuild",
    "ffmpeg-static",
    "lefthook",
    "node-crc",
    "unrs-resolver"
  ],
  "patchedDependencies": {
    "mem0ai@2.2.1": "patches/mem0ai@2.2.1.patch"
  }
}
